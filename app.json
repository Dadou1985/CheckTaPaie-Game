{
  "expo": {
    "name": "checkTaPaie-Game",
    "slug": "checkTaPaie-Game",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/CTP_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/CTP_splash-screen.png",
      "resizeMode": "cover",
      "backgroundColor": "transparent"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/CTP_logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/CTP_logo.png",
      "display": "fullscreen"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
